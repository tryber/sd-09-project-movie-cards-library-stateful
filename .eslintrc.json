{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "trybe-frontend",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "sonarjs"
  ],
  "parser": "babel-eslint"
}
